        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler" enabled="true">
          <stringProp name="BeanShellSampler.query">import java.io.FileWriter;
import java.util.Arrays;
import java.nio.charset.Charset;

//import java.io.Writer;
//import java.util.List;

//Default separator
char SEPARATOR = &apos;,&apos;;

//function write line in csv
public void writeLine(FileWriter writer, String[] params, char separator) {
    StringBuilder stringBuilder = new StringBuilder();
    stringBuilder.append(String.join(&quot;,&quot;, params));
    // add newline
    stringBuilder.append(&quot;\n&quot;);

   //add to file the line
   log.info(stringBuilder.toString());
   try {
   writer.append(stringBuilder.toString());
   } catch(Exception e) {
	log.info(e.toString() + e.getMessage());
   }
}

//get path of csv file (creates new one if its not exists)
String csvFile = &quot;${walsigmsgCSV}&quot;;

log.info(&quot;${wallet}&quot;);
log.info(&quot;${signature}&quot;);
log.info(&quot;${message}&quot;);

String[] params = {&quot;${wallet}&quot;, &quot;${signature}&quot;, &quot;${message}&quot;};

FileWriter fileWriter;
if ( &quot;${walsigmsgCSVFirstLine}&quot; == &quot;yes&quot;) {
	fileWriter = new FileWriter(csvFile, Charset.forName(&quot;UTF-8&quot;), false);
	fileWriter.write(&quot;wallet,signature,message\n&quot;);
	vars.put(&quot;walsigmsgCSVFirstLine&quot;, &quot;no&quot;);
} else {
	fileWriter = new FileWriter(csvFile, Charset.forName(&quot;UTF-8&quot;), true);
}


writeLine(fileWriter, params, SEPARATOR);

//Flush and close
fileWriter.flush();
fileWriter.close();</stringProp>
          <stringProp name="BeanShellSampler.filename"></stringProp>
